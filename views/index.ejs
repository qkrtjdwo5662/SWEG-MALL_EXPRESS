      <%-include('./include/nav.ejs')%>
      <!-- Swiper -->
      <div class="swiper mySwiper">
        <div class="swiper-wrapper">
          <div class="swiper-slide">
            <div class="main-banner">
              <div class="title">SW&nbsp&nbsp&nbsp&nbsp&nbspEG</div>
              <img src="/img/main-banner.png" alt="" />
            </div>
          </div>
