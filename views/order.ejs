  async function orderRequest(query){
    const res = await fetch(`http://localhost:4000/order/order-request${query}`, { 
      method: 'post',
      headers: {
        'Content-type': 'application/json'
      },
    });

    if(res.status == 200){
      return alert("주문 요청 완료");
    }else alert("잠시 후 다시 시도해주세요");
  }


